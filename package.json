{
  "name": "ronb-node",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:prod:newscache": "node dist/apps/newscache/main.js",
    "start:prod:ronb-node": "npm run migration:run && node dist/apps/ronb-node/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/ronb-node/test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:generate": "npm run typeorm -- migration:generate -d datasource.ts",
    "migration:run:dev": "npm run typeorm -- migration:run -d datasource.ts && npm run migration:build",
    "migration:revert:dev": "npm run typeorm -- migration:revert -d datasource.ts",
    "migration:run": "npx typeorm  migration:run -d dist/libs/shared/live-config/datasource.js",
    "migration:revert": "npx typeorm migration:revert -d dist/libs/shared/live-config/datasource.js",
    "migration:build": "npx tsc libs/shared/src/migrations/* --outDir libs/shared/src/live-migrations/",
    "loadAddress": "node src/config/loadAddress.ts",
    "proto:news": "protoc --plugin=node_modules/.bin/protoc-gen-ts_proto -I=./libs/shared/src/common/proto --ts_proto_out=libs/shared/src/common/proto libs/shared/src/common/proto/news.proto --ts_proto_opt=nestJs=true --ts_proto_opt=fileSuffix=.pb",
    "proto:recommender": "protoc --plugin=node_modules/.bin/protoc-gen-ts_proto -I=./libs/shared/src/common/proto --ts_proto_out=libs/shared/src/common/proto libs/shared/src/common/proto/recommendation.proto --ts_proto_opt=nestJs=true --ts_proto_opt=fileSuffix=.pb",
    "proto:all": "npm run proto:news && npm run proto:recommender"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.154.0",
    "@grpc/grpc-js": "^1.8.13",
    "@grpc/proto-loader": "^0.7.6",
    "@nestjs/apollo": "^10.0.17",
    "@nestjs/cache-manager": "^1.0.0",
    "@nestjs/common": "^9.3.11",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.3.11",
    "@nestjs/graphql": "^10.0.18",
    "@nestjs/microservices": "^9.4.0",
    "@nestjs/platform-express": "^9.0.2",
    "@nestjs/serve-static": "^3.0.0",
    "@nestjs/typeorm": "^9.0.0",
    "apollo-server-express": "^3.10.0",
    "async": "^3.2.4",
    "aws-sdk": "^2.1201.0",
    "body-parser": "^1.20.0",
    "cache-manager": "^5.2.0",
    "cache-manager-redis-store": "^3.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cookie-parser": "^1.4.6",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.0.1",
    "firebase-admin": "^11.0.1",
    "fs-capacitor": "^6.2.0",
    "geojson": "^0.5.0",
    "geojson-validation": "^1.0.2",
    "graphql": "^16.5.0",
    "graphql-relay": "^0.10.0",
    "graphql-scalars": "^1.20.1",
    "graphql-tools": "^8.3.0",
    "graphql-type-json": "^0.3.2",
    "graphql-upload": "^13.0.0",
    "mongoose": "^6.5.1",
    "pg": "^8.9.0",
    "pg-format": "^1.0.4",
    "redis": "^4.3.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "string-strip-html": "^4.3.0",
    "typeorm": "^0.3.7"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.1",
    "@nestjs/testing": "^9.0.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
    "@types/geojson": "^7946.0.10",
    "@types/graphql-relay": "^0.7.0",
    "@types/graphql-upload": "^8.0.11",
    "@types/jest": "27.5.0",
    "@types/node": "^16.18.23",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.0.3",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "sqlite3": "^5.0.11",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.1",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "ts-proto": "^1.146.0",
    "tsconfig-paths": "4.0.0",
    "typeorm-model-generator": "^0.4.6",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app/shared(|/.*)$": "<rootDir>/libs/shared/src/$1"
    }
  }
}
